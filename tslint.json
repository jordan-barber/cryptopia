{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {},
  "rules": {
    "eofline": false,
    "quotemark": [true, "double"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "import-name": [
      true,
      {
        "logger": "Logger",
        "config": "Config"
      }
    ]
  },
  "rulesDirectory": []
}
